---
import Base from "@layouts/Base.astro";
import PageTitle from "@layouts/partials/PageTitle.astro";
import Wrapper from "@layouts/partials/Wrapper.astro";

const { title } = Astro.props;
---

<Base title={title}>
	<slot name="header">
		<PageTitle>{title}</PageTitle>
	</slot>
	<Wrapper>
		<slot />
	</Wrapper>
</Base>