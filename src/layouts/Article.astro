---
import Base from "@layouts/Base.astro";
import PageTitle from "@layouts/partials/PageTitle.astro";
import Wrapper from "@layouts/partials/Wrapper.astro";

const { frontmatter } = Astro.props;
---

<Base title={frontmatter.title}>
	<PageTitle>{frontmatter.title}</PageTitle>
	<Wrapper class="leading-relaxed">
		<slot />
	</Wrapper>
</Base>